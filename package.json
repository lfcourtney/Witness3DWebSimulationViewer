{
  "name": "witness3dwebsimulationviewer",
  "private": true,
  "version": "0.0.7",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --base=./",
    "preview": "vite preview",
    "prepare": "husky",
    "incrementPatch": "npm version --no-git-tag-version --force patch && git add package.json package-lock.json"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "eslintConfig": {
    "extends": [
      "prettier"
    ]
  },
  "devDependencies": {
    "@babylonjs/core": "^7.26.4",
    "@babylonjs/inspector": "^7.26.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "vite": "^5.4.1"
  }
}

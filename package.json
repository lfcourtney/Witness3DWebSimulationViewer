{
  "name": "witness3dwebsimulationviewer",
  "private": true,
  "version": "0.0.36",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4000",
    "build": "tsc && vite build --base=./",
    "preview": "vite preview",
    "prepare": "husky",
    "incrementPatch": "npm version --no-git-tag-version --force patch && git add package.json package-lock.json",
    "test": "vitest run --coverage"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.ts": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "prettier"
    ]
  },
  "devDependencies": {
    "@babylonjs/core": "^7.26.4",
    "@babylonjs/gui": "^7.28.0",
    "@babylonjs/inspector": "^7.26.4",
    "@eslint/js": "^9.11.1",
    "@types/node": "^22.7.4",
    "@vitest/coverage-v8": "^2.1.2",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "fast-xml-parser": "^4.5.0",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.1",
    "vitest": "^2.1.2"
  }
}
